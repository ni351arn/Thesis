      \strng{namehash}{c820c3d804b25e5ca97fcb4157c80f9e}
      \strng{fullhash}{c820c3d804b25e5ca97fcb4157c80f9e}
      \strng{fullhashraw}{c820c3d804b25e5ca97fcb4157c80f9e}
      \strng{bibnamehash}{c820c3d804b25e5ca97fcb4157c80f9e}
      \strng{authorbibnamehash}{c820c3d804b25e5ca97fcb4157c80f9e}
      \strng{authornamehash}{c820c3d804b25e5ca97fcb4157c80f9e}
      \strng{authorfullhash}{c820c3d804b25e5ca97fcb4157c80f9e}
