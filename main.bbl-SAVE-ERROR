    \entry{enemosahEnhancingDevOpsEfficiency2025}{article}{}{}
      \name{author}{1}{}{%
        {{hash=4e3ed1028610a091d515ed60aa882671}{%
           family={Enemosah},
           familyi={E\bibinitperiod},
           given={Aliyu},
           giveni={A\bibinitperiod}}}%
      }
      \strng{namehash}{4e3ed1028610a091d515ed60aa882671}
      \strng{fullhash}{4e3ed1028610a091d515ed60aa882671}
      \strng{fullhashraw}{4e3ed1028610a091d515ed60aa882671}
      \strng{bibnamehash}{4e3ed1028610a091d515ed60aa882671}
      \strng{authorbibnamehash}{4e3ed1028610a091d515ed60aa882671}
      \strng{authornamehash}{4e3ed1028610a091d515ed60aa882671}
      \strng{authorfullhash}{4e3ed1028610a091d515ed60aa882671}
